  <transform>
    <name>Aprovador_Pedido</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>mysql_UNO</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>
SELECT Distinct
vd_pedido.cod_pedido,
date(vd_pedido.dt_implant) dt_implant,
vd_pedido.cod_colaborador_implant,
date(vd_pedido.dt_alteracao) dt_alteracao,
date(vd_pedido.dt_aprovacao) dt_aprovacao,
date('1900-01-01') dt_pendentefin,
date('1900-01-01')  dt_pendentecom,
vd_pedido.cod_colaborador_alteracao,
sg_colaborador.nome_colaborador nome_alteracao,
sg2.nome_colaborador nome_aprovacao,
'' nome_pendentefin,
'' nome_pendentecom
FROM vd_pedido left JOIN sg_colaborador
ON sg_colaborador.cod_colaborador = vd_pedido.cod_colaborador_alteracao
LEFT JOIN vd_pedido_hist h ON h.cod_pedido = vd_pedido.cod_pedido   AND h.tp_historico = 80
AND date(vd_pedido.dt_aprovacao) = date(h.dt_referencia)
AND h.descricao LIKE '%Para: Aprovado%'
LEFT JOIN sg_colaborador sg2 ON sg2.cod_colaborador = h.cod_colaborador
WHERE vd_pedido.dt_implant >= '2023-01-01'
union all
SELECT Distinct
vd_pedido.cod_pedido,
date(vd_pedido.dt_implant) dt_implant,
vd_pedido.cod_colaborador_implant,
date(vd_pedido.dt_alteracao) dt_alteracao,
date('1900-01-01') dt_aprovacao,
date(h.dt_referencia) dt_pendentefin,
date('1900-01-01')  dt_pendentecom,
vd_pedido.cod_colaborador_alteracao,
sg_colaborador.nome_colaborador nome_alteracao,
'' nome_aprovacao,
sg2.nome_colaborador nome_pendentefin,
'' nome_pendentecom
FROM vd_pedido left JOIN sg_colaborador
ON sg_colaborador.cod_colaborador = vd_pedido.cod_colaborador_alteracao
inner JOIN vd_pedido_hist h ON h.cod_pedido = vd_pedido.cod_pedido   AND h.tp_historico = 80
AND h.descricao LIKE '%Para: Pendente Financeiro%'
LEFT JOIN sg_colaborador sg2 ON sg2.cod_colaborador = h.cod_colaborador
WHERE vd_pedido.dt_implant >= '2023-01-01'
union all
SELECT Distinct
vd_pedido.cod_pedido,
date(vd_pedido.dt_implant) dt_implant,
vd_pedido.cod_colaborador_implant,
date(vd_pedido.dt_alteracao) dt_alteracao,
date('1900-01-01') dt_aprovacao,
date('1900-01-01') dt_pendentefin,
date(h.dt_referencia)  dt_pendentecom,
vd_pedido.cod_colaborador_alteracao,
sg_colaborador.nome_colaborador nome_alteracao,
'' nome_aprovacao,
'' nome_pendentefin,
sg2.nome_colaborador nome_pendentecom
FROM vd_pedido left JOIN sg_colaborador
ON sg_colaborador.cod_colaborador = vd_pedido.cod_colaborador_alteracao
inner JOIN vd_pedido_hist h ON h.cod_pedido = vd_pedido.cod_pedido   AND h.tp_historico = 80
AND h.descricao LIKE '%Para: Pendente Comercial%'
LEFT JOIN sg_colaborador sg2 ON sg2.cod_colaborador = h.cod_colaborador
WHERE vd_pedido.dt_implant >= '2023-01-01'</sql>
