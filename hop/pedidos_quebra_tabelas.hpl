  <transform>
    <name>Staus_Pedido_Varejo_UNO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Athena_AWS_Kami</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>select distinct
    case 
        when p.dt_implante_pedido is not null and try(date_parse(p.dt_implante_pedido, '%Y-%m-%d %H:%i:%s')) is not null 
        then date(try(date_parse(p.dt_implante_pedido, '%Y-%m-%d %H:%i:%s'))) 
        else null 
    end as dt_implante_pedido,
    case 
        when p.dt_faturamento is not null and try(date_parse(p.dt_faturamento, '%Y-%m-%d %H:%i:%s')) is not null 
        then date(try(date_parse(p.dt_faturamento, '%Y-%m-%d %H:%i:%s'))) 
        else null 
    end as dt_faturamento,
    case 
        when p.dt_cancelamento is not null and try(date_parse(p.dt_cancelamento, '%Y-%m-%d %H:%i:%s')) is not null 
        then date(try(date_parse(p.dt_cancelamento, '%Y-%m-%d %H:%i:%s'))) 
        else null 
    end as dt_cancelamento,
    case 
        when p.dt_alteracao is not null and try(date_parse(p.dt_alteracao, '%Y-%m-%d %H:%i:%s')) is not null 
        then date(try(date_parse(p.dt_alteracao, '%Y-%m-%d %H:%i:%s'))) 
        else null 
    end as dt_alteracao,
    p.cod_pedido,
    p.cod_cliente,
    p.nome_cliente,
    p.situacao_pedido,
    p.nop,
    p.descricao,
    p.desc_abreviada,
    p.desc_abrev_cfop as cfop,
    case when p.faturamento_empresa_fantasia = '0' and p.nome_empresa_pedido = 'MOVEMENT Distribibuidora e Comercio de CosmÃ©ticos Eireli' Then 'MOVEMENT' 
         when p.faturamento_empresa_fantasia = '0' Then concat(replace(p.nome_empresa_pedido,'FILIAL',''),'  ',p.uf_empresa_pedido) 
         when p.nome_empresa_pedido = 'MOVEMENT DISTRIBUIDORA E COMERCIO. DE COSMETICOS LTDA RJ' Then 'MOVEMENT RJ'
        Else replace(p.faturamento_empresa_fantasia,'FILIAL','') 
    end empresa,
    p.nome_empresa_pedido empresa_pedido,
    p.uf_empresa_faturamento as uf_faturamento,
    p.uf_empresa_pedido as uf_pedido,
    v.operacoes_internas,
    count(p.cod_produto) qtd_itens,
    count(distinct p.cod_produto) qtd_sku,
    count(distinct p.marca) qtd_marcas,
    sum(p.preco_desconto_rateado) vlr_pedido
from 	databeautykami.tbl_varejo_pedidos p left join databeautykami.vw_distribuicao_pedidos v
	ON v.cod_pedido = p.cod_pedido
where p.situacao_pedido not in ('0', '1', '15') 
  and dt_implante_pedido is not null 
  and try(date_parse(dt_implante_pedido, '%Y-%m-%d %H:%i:%s')) >= date('2023-01-01') 
group by 
    p.dt_implante_pedido,
    p.dt_faturamento,
    p.dt_cancelamento,
    p.dt_alteracao,
    p.cod_pedido,
    p.cod_cliente,
    p.nome_cliente,
    p.situacao_pedido,
    p.nop,
    p.descricao,
    p.desc_abreviada,
    p.desc_abrev_cfop,
    p.faturamento_empresa_fantasia,
    p.nome_empresa_pedido,
    p.uf_empresa_faturamento,
    p.uf_empresa_pedido,
    v.operacoes_internas ;</sql>
