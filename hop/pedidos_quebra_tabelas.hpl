    <connection>mysql_UNO</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT 
		cd_cliente.cod_cliente,
		cd_cliente.cod_empresa,
		cd_empresa.nome_fantasia empresa,
		Case 	when cd_cliente.situacao = 0  Then 'Inativo'
				when cd_cliente.situacao = 1  Then 'Ativo'
				when cd_cliente.situacao = 3  Then 'Prospect'
				when cd_cliente.situacao = 9  Then 'Bloqueado'
				when cd_cliente.situacao = 10 Then 'Cancelado'
      		ELSE 'NSA'
		END AS situacao,				
		cd_cliente.nome_cliente,
		Case 	when (cd_cliente.cnpj IS not NULL) THEN cd_cliente.cnpj
		      when (cd_cliente.cnpj IS NULL AND cd_cliente.cpf IS NOT NULL) then cd_cliente.cpf 
				when (cd_cliente.cnpj IS NULL AND cd_cliente.cpf IS NULL AND cd_cliente_contato.cpf IS NOT NULL ) then cd_cliente_contato.cpf
				ELSE cd_cliente.cod_cliente 
		END AS cpfcnpj,
		cd_grupo_cliente.desc_abrev grupo_cliente,
		cd_cliente.limite_credito
FROM 	cd_cliente 
		LEFT JOIN cd_grupo_cliente ON cd_grupo_cliente.cod_grupo_cliente = cd_cliente.cod_grupo_cliente
		LEFT JOIN cd_empresa ON cd_empresa.cod_empresa = cd_cliente.cod_empresa
		LEFT JOIN cd_cliente_contato ON cd_cliente_contato.cod_cliente = cd_cliente.cod_cliente
WHERE cd_cliente.situacao != 10
</sql>
