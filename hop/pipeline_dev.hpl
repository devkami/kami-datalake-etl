    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT distinct
	cd_cliente.cod_cliente,
	CASE 	WHEN `cd_cliente`.`cnpj` IS NOT NULL THEN `cd_cliente`.`cnpj` 
		WHEN `cd_cliente`.`cnpj` IS NULL AND `cd_cliente`.`cpf` IS NOT NULL THEN `cd_cliente`.`cpf` 
		ELSE `vd_pedido`.`cod_cliente` 
	END AS `cpfcnpj`,
	sg_colaborador.cod_colaborador AS cod_colaborador_atual,
	sg_colaborador.nome_colaborador AS nome_colaborador_atual
FROM vd_pedido inner JOIN cd_cliente ON cd_cliente.cod_cliente = vd_pedido.cod_cliente
INNER JOIN sg_colaborador ON sg_colaborador.cod_colaborador = cd_cliente.cod_colaborador</sql>
